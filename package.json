    "scripts": {
        "start": "env-cmd -f .env.demo react-scripts start",
        "start-production": "env-cmd -f .env.production react-scripts start",
        "start-local": "env-cmd -f .env.buildLocal react-scripts start",
        "build": "npm-run-all --parallel build:*",
        "build:build-development": "set BUILD_PATH=./build/development&& env-cmd -f .env.demo react-scripts build",
        "build:build-production": "set BUILD_PATH=./build/production&& env-cmd -f .env.production react-scripts build ./build/production",
        "build:build-local": "set BUILD_PATH=./build/local&& env-cmd -f .env.buildLocal react-scripts build ./build/local",
        "analyze": "source-map-explorer 'build/static/**/*.*'",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint": "eslint --fix src"
    },
